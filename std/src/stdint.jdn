#include <stdint.h>

in std{
implement Int {

	Int(int value) {
		this->value = value;
	}

	int getValue() {
		return this->value;
	}

	void setValue(int value) {
		this->value = value;
	}

	std::String toString() {

		int last = this->value % 10;
		Int tail = new Int(last);
		int first = this->value / 10;
		if(first > 0) {
			Int head = new Int(first);
			return head->toString()->concat(tail);
		} else {
			return new std::String()->concat((char) ('0' + tail->getValue()));
		}


	}

}
}
in std {
	class object {

			private char* name;

			virtual int hashcode(void);
			virtual int equals(object* other);
	};
}

int main() {
		std::object id;

		return 0;
}


in std {
	implement object {
		int hashcode(void) {
        		return (int) this;
        }

        int equals(object* other) {
        	return this == other;
        }
	}
}



